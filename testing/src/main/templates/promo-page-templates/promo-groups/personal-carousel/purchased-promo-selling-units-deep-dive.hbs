{
  "template_id": "purchased-promo-selling-units-deep-dive",
  "layout": {
    "id": "purchased-promo-selling-units-deep-dive",
    "header": {
      "title": "{{localize COPY.headerTitle}}"
    },
    "body": {
      "id": "root",
      "type": "BLOCK",
      "layout": {
        "type": "FLOW",
        "axis": "VERTICAL"
      },
      "size": {
        "crossAxis": "12g"
      },
      "children": [
        {
          "id": "promo-group-purchased-vertical-selling-unit-tiles",
          "type": "BLOCK",
          "layout": {
            "type": "FLOW",
            "axis": "VERTICAL",
            "spacing": {
              "mainAxis": 4,
              "crossAxis": 4
            },
            "padding": {
              "left": 8,
              "right": 8,
              "top": 4,
              "bottom": 4
            }
          },
          "size": {
            "crossAxis": "12g"
          },
          "children": [
            {{#each purchasedPromoSellingUnits as | purchasedPromoSellingUnit |}}
              {{{template 'core-vertical-selling-unit-tile-template' sellingUnit=purchasedPromoSellingUnit}}},
            {{/each}}
          ]
        }
      ]
    }
  },
  "context": {
    "purchasedPromoSellingUnits": {
      "$query": "SELECT * FROM purchased_promo_selling_units WHERE id IN ('irrelevant')"
    },
    "COPY": {
      "headerTitle": {
        "nl": "Jouw acties",
        "de": "Deine Angebote",
        "fr": "Vos promos"
      }
    }
  },
  "metadata": {}
}
